{
    "name": "joppedc/sylius-better-seo-plugin",
    "type": "sylius-plugin",
    "keywords": ["sylius", "sylius-plugin"],
    "description": "Support for SEO metadata on products",
    "license": "MIT",
    "require": {
        "php": "^7.2 || ^8.0",
        "sylius/sylius": "^1.4"
    },
    "require-dev": {
        "behat/behat": "^v3.10.0",
        "behat/mink": "^v1.9.0",
        "behat/mink-browserkit-driver": "^v1.4.0",
        "behat/mink-extension": "^2.3.1",
        "behat/mink-selenium2-driver": "^1.5",
        "friends-of-behat/mink-debug-extension": "^2.0",
        "friends-of-behat/page-object-extension": "^0.3",
        "friends-of-behat/suite-settings-extension": "^1.1",
        "friends-of-behat/symfony-extension": "^2.2",
        "friends-of-behat/variadic-extension": "^1.4",
        "phpspec/phpspec": "^6.3.1",
        "phpstan/phpstan-doctrine": "^0.12.44",
        "phpstan/phpstan": "^0.12.99",
        "phpstan/phpstan-webmozart-assert": "^0.12.16",
        "phpunit/phpunit": "^9.5",
        "sensiolabs/security-checker": "^5.0",
        "sylius-labs/coding-standard": "^v3.0.0",
        "symfony/browser-kit": "^3.4|^4.1",
        "symfony/debug-bundle": "^3.4|^4.1",
        "symfony/dotenv": "^4.2",
        "symfony/intl": "^3.4|^4.1",
        "symfony/web-profiler-bundle": "^3.4|^4.1",
        "symfony/web-server-bundle": "^3.4|^4.1"
    },
    "conflict": {
        "symfony/symfony": "4.1.8",
        "symfony/browser-kit": "4.1.8",
        "symfony/dependency-injection": "4.1.8",
        "symfony/dom-crawler": "4.1.8",
        "symfony/routing": "4.1.8"
    },
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "JoppeDc\\SyliusBetterSeoPlugin\\": "src/",
            "Tests\\JoppeDc\\SyliusBetterSeoPlugin\\": "tests/"
        }
    },
    "autoload-dev": {
        "classmap": ["tests/Application/Kernel.php"]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
